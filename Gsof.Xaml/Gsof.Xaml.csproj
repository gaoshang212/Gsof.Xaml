<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net45;net46;net5.0-windows;net6.0-windows</TargetFrameworks>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<Authors>gaoshang212</Authors>
		<LangVersion>10.0</LangVersion>
		<PackageProjectUrl>https://github.com/gaoshang212/Gsof.Xaml</PackageProjectUrl>
		<RepositoryUrl>https://github.com/gaoshang212/Gsof.Xaml.git</RepositoryUrl>
		<AssemblyVersion>0.3.4</AssemblyVersion>
		<VersionPrefix>0.3.4</VersionPrefix>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Gsof" Version="0.3.1" />
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.39" />
	</ItemGroup>

</Project>
